/*place global styles here */
html,
body {
	@apply h-full;
	@apply m-0;
	@apply p-0;
}

@font-face {
	font-family: 'Abril Fatface';
	src: url('/fonts/AbrilFatface.ttf');
	font-display: swap;
}
:root {
    --theme-font-family-base: 'Abril Fatface', Abril Fatface;
    --theme-font-family-heading: 'Abril Fatface', Abril Fatface;
    /* ... */
}